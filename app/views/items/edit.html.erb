<h4>商品編集</h4>
  <%= form_with model: @items_edit_path do |f| %>
  <p>商品画像
  <%= f.file_field :image, accept: "image*/" %></p>
  <p>商品名
  <%= f.text_field :name %></p>
  <p>商品説明
  <%= f.text_area :introduction %></p>
  <p>税抜価格
  <%= f.text_field :price %>円</p>
  <%= f.submit '変更を保存', class: 'btn btn-success' %>
  <% end %>